import org.springframework.web.bind.annotation.RequestMapping;

public class SampleController3 {

	private static final Logger logger = LoggerFactory.getLogger(SampleController3.class);	 //복붙	

	
	
	@RequestMapping("/doG")
	public String doG() {
		logger.info("called doG....");
		return "testG";




}
