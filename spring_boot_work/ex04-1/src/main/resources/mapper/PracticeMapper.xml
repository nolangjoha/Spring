<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.docmall.demo.mapper.PracticeMapper">

	<select id="list" resultType="com.docmall.demo.domain.PracticeVO">
		<![CDATA[
		SELECT
		    bno, 
		    title, 
		    content, 
		    writer, 
		    regdate, 
		    updatedate, 
		    viewcount
		FROM
		    practice
		WHERE 
			ROWNUM <=10
		]]>	
	</select>



</mapper>

